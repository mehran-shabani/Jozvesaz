# Base URL for the FastAPI backend
API_BASE_URL=http://localhost:8000

# Public fallback URLs (override if the frontend needs to access the API from the browser)
NEXT_PUBLIC_API_BASE_URL=http://localhost:8000

# Cookie names shared with the FastAPI backend
NEXT_PUBLIC_ACCESS_TOKEN_COOKIE_NAME=access_token
NEXT_PUBLIC_REFRESH_TOKEN_COOKIE_NAME=refresh_token
